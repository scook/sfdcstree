    <validationRules>
        <fullName>Dollar_Estimate_High_Less_Than_Zero</fullName>
        <active>true</active>
        <description>Ensure the dollar estimate high is not less than 0</description>
        <errorConditionFormula>NOT(ISBLANK(Dollar_Estimate_High__c)) &amp;&amp;
  Dollar_Estimate_High__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Dollar_Estimate_High__c</errorDisplayField>
        <errorMessage>The Dollar Estimate (High) cannot be less than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dollar_Estimate_Low_Less_Med_and_High</fullName>
        <active>true</active>
        <description>Ensure the dollar estimate low is less than the medium and high</description>
        <errorConditionFormula>NOT(ISBLANK(Dollar_Estimate_Low__c)) &amp;&amp;
((NOT(ISBLANK(Dollar_Estimate_Medium__c)) &amp;&amp;
  Dollar_Estimate_Low__c &gt; Dollar_Estimate_Medium__c) ||
((NOT(ISBLANK(Dollar_Estimate_High__c)) &amp;&amp;
  Dollar_Estimate_Low__c &gt; Dollar_Estimate_High__c)))</errorConditionFormula>
        <errorDisplayField>Dollar_Estimate_Low__c</errorDisplayField>
        <errorMessage>The Dollar Estimate (Low) cannot be more than the Dollar Estimate (Medium) or Dollar Estimate (High)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dollar_Estimate_Low_Less_Than_Zero</fullName>
        <active>true</active>
        <description>Ensure the dollar estimate low is not less than 0</description>
        <errorConditionFormula>NOT(ISBLANK(Dollar_Estimate_Low__c)) &amp;&amp;
  Dollar_Estimate_Low__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Dollar_Estimate_Low__c</errorDisplayField>
        <errorMessage>The Dollar Estimate (Low) cannot be less than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dollar_Estimate_Med_Less_Than_Zero</fullName>
        <active>true</active>
        <description>Ensure the dollar estimate medium is not less than 0</description>
        <errorConditionFormula>NOT(ISBLANK(Dollar_Estimate_Medium__c)) &amp;&amp;
  Dollar_Estimate_Medium__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Dollar_Estimate_Medium__c</errorDisplayField>
        <errorMessage>The Dollar Estimate (Medium) cannot be less than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dollar_Estimate_Med_More_High</fullName>
        <active>true</active>
        <description>Ensure the dollar estimate medium is less than high</description>
        <errorConditionFormula>NOT(ISBLANK(Dollar_Estimate_Medium__c)) &amp;&amp;
((NOT(ISBLANK(Dollar_Estimate_High__c)) &amp;&amp;
  Dollar_Estimate_Medium__c &gt; Dollar_Estimate_High__c))</errorConditionFormula>
        <errorDisplayField>Dollar_Estimate_Medium__c</errorDisplayField>
        <errorMessage>The Dollar Estimate (Medium) cannot be more than the Dollar Estimate (High)</errorMessage>
    </validationRules>
